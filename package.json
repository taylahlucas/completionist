  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "ios": "ENVFILE=.env watchman watch-del-all && react-native run-ios",
    "clean-ios": "rm -rf node_modules package-lock.json yarn.lock && cd ios && rm -rf Pods Podfile.lock build && cd .. && rm -rf ~/Library/Developer/Xcode/DerivedData && npm install && rm -rf $TMPDIR/metro* && cd ios && pod install",
    "kill": "pkill -f 'react-native' && pkill -f 'Simulator'",
    "lint": "eslint .",
    "start": "ENVFILE=.env watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "generate-locale": "babel-node --presets @babel/preset-env --extensions .ts scripts/locale.ts",
    "generate:api": "orval"
  },
